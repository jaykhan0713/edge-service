{
  "family": "jay-platform-edge-service-prod",
  "taskRoleArn": "arn:aws:iam::454842419646:role/jay-platform-edge-task-role-prod",
  "executionRoleArn": "arn:aws:iam::454842419646:role/jay-platform-edge-task-execution-prod",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "app",
      "image": "__APP_IMAGE__",
      "cpu": 0,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "dockerLabels": {},
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/jay-platform/prod/edge-service",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    },
    {
      "name": "adot-collector",
      "image": "454842419646.dkr.ecr.us-west-2.amazonaws.com/jay-platform/adot-collector:git-initial",
      "cpu": 0,
      "essential": false,
      "portMappings": [],
      "environment": [
        {
          "name": "SERVICE_NAME",
          "value": "edge-service-prod"
        },
        {
          "name": "AWS_REGION",
          "value": "us-west-2"
        },
        {
          "name": "AMP_REMOTE_WRITE_ENDPOINT",
          "value": "https://aps-workspaces.us-west-2.amazonaws.com/workspaces/ws-2eea101e-6aad-4579-a198-d29198b23426/api/v1/remote_write"
        },
        {
          "name": "ENV",
          "value": "prod"
        },
        {
          "name": "APP_PORT",
          "value": "8080"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "dockerLabels": {},
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/jay-platform/prod/edge-service-adot",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "volumes": [],
  "tags": [
    {
      "key": "Project",
      "value": "jay-platform"
    },
    {
      "key": "ManagedBy",
      "value": "codepipeline"
    },
    {
      "key": "Env",
      "value": "prod"
    },
    {
      "key": "Name",
      "value": "jay-platform-edge-taskdef-prod"
    }
  ]
}
